<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-app-center" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>cordova-plugin-app-center</name>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="AppCenterPlugin">
                <param name="ios-package" value="AppCenterPlugin" />
                <param name="onload" value="true" />
            </feature>
        </config-file>
        <source-file src="src/ios/AppCenterPlugin.m" />
        <header-file src="src/ios/AppCenterPlugin.h" />
        <framework src="AppCenter" type="podspec"  spec="~> 1.1.0" />
    </platform>
</plugin>